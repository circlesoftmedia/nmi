<div id="page-wrapper" >
  <div id="page-inner">
    <div class="row">
      <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Basic Form Elements
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-lg-6">            
                          <%= form_for(@user, :url => webmaster_user_path(@user), :html => { :method => :put }) do |f| %>
                          
                            <div class="form-group">
                              <label><span class="req">*</span>First Name</label> 
                              <%= f.text_field :first_name %> 
                            </div> 
                            <div class="form-group">
                              <label><span class="req">*</span>Last Name </label> 
                              <%= f.text_field :last_name %>   
                            </div>
                            <div class="form-group">
                              <label><span class="req">*</span>Company Name</label> 
                              <%= f.text_field :company_name %>   
                            </div>
                            
                            <div class="form-group">
                              <label><span class="req">*</span>Phone </label> 
                              <%= f.text_field :phone %>  
                            </div>
                            <div class="form-group">
                              <label><span class="req">*</span>Mobile </label> 
                              <%= f.text_field :mobile %>  
                            </div>
                            <div class="form-group">
                              <label><span class="req">*</span>Email</label> 
                              <%= f.email_field :email, :autofocus => true  %>    
                            </div>
                            
                            <div class="form-group">
                              <label>Address 1 </label> 
                              <%= f.text_field :address_1 %>
                            </div>
                            <div class="form-group">
                              <label>Address 2 </label> 
                              <%= f.text_field :address_2 %>
                            </div>
                            <div class="form-group">
                              <label>City</label> 
                              <%= f.text_field :city %>
                            </div>
                            
                            <div class="form-group">
                              <label>State</label> 
                              <%= f.text_field :state %>
                            </div>
                            <div class="form-group">
                              <label>Zip/Postal Code </label> 
                              <%= f.text_field :city %>
                            </div>
                            <div class="form-group">
                              <label>Country</label> 
                              <%= f.text_field :country %> 
                            </div>
                      
                            <div class="form-item">
                              <label>Business Segment/Type </label> 
                              <%= f.text_field :business_type %> 
                            </div>
                            <div class="form-group">
                              <label>Type of User </label> 
                              <%= f.text_field :user_type %> 
                            </div>
                            <div class="form-group">
                              <label>How did you here about NMI</label> 
                              <%= f.text_field :hear_from %>   
                            </div>
                            
                            <div class="form-group">
                              <label>Security Code<br>
                              </label> 
                              <%= f.text_field :security_code %>     
                            </div>

                            <div class="form-group">
                              <label>User Role<br>
                              </label> 
                              <%= f.text_field :role %>     
                            </div>

                            <div class="form-group">
                              <div class="checkbox">
                                <%= f.check_box :subscribe_to_updates %>
                                <span>LinnkedinI would like to recieve the latest news and offers from NMI.</span>
                              </div>
                              <div class="checkbox">
                                <%= f.check_box :toc %>
                                <span>I have read and agree to the terms of use and privacy policy.</span>
                              </div>
                              <span class="clr"></span>
                            </div>
                            <input type="submit" value="Submit" title="Submit" class="buttons form-submit-btn" />
                          <% end %>
      </div> 
    </div>
  </div>
</div>