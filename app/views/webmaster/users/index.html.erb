
<div id="page-wrapper">
  <div id="page-inner">
    <div class="row">
      <div class="col-md-8 col-sm-12 col-xs-12">
        <div class="panel panel-default">
          <div class="panel-heading">
             User Lists 
          </div> 
          <div class="panel-body">
            <div class="table-responsive">
              <table class="table table-striped table-bordered table-hover">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Email ID.</th>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>User Role</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>
                    <% @users.each do |user|%>
	                    <tr>
	                        <td><%= user.id %></td>
	                        <td><%= user.email %></td>
	                        <td><%= user.first_name %></td>
	                        <td><%= user.last_name %></td>
	                        <td><%= user.role %></td>
	                        <td><%= link_to 'show', webmaster_user_path(user)%></td>
	                        <td><%= link_to 'edit', edit_webmaster_user_path(user) %></td>
	                        <td><%= link_to 'Destroy', webmaster_user_path(user),method: :delete, data: { confirm: 'Are you sure?' } %></td>
	                    </tr>
                    <% end %>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>