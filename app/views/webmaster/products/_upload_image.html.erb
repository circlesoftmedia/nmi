<%= form_for Image.new,:url=>upload_image_webmaster_images_path, :id=> 'new_image', remote: true  do |f| %>
  <%= f.hidden_field :product_token, value: @product.token %>
    <span class="btn btn-mini fileinput-button">
      <i class="icon-plus"></i>
      	<button type="submit" class="btn btn-default">Upload photos</button>
  			<%= f.file_field :image, multiple: true, name: "image[image]" %>
    </span>
    <br/>
<% end %>